#! /bin/hilbish
local ansikit = require 'ansikit'

if os.getenv 'TERM' == 'linux' then
	local colors = {
		'2d2d2d',
		'd04645',
		'77b755',
		'f4c94d',
		'3E89E7',
		'e25e9c',
		'5EDFE2',
		'f0f0f0',
		'999999',
		'DF5554',
		'8ACC68',
		'FDD565',
		'4993F0',
		'EB6AA6',
		'6FEBEE',
		'ffffff'
	}

	for i = 1, 16 do
		print('\27]P' .. string.format("%X", i - 1)
		.. colors[i])
	end
	ansikit.clear()
	ansikit.cursorTo(0, 0)
end
